{
    "name": "hybrid",
    "version": "1.0.0",
    "description": "Real world example of hybrid recommender",
    "author": "Vladimír Fojtík <fojtik.v@gmail.com>",
    "private": true,
    "scripts": {
        "client:dev": "nuxt",
        "client:build": "nuxt build",
        "client:start": "nuxt start",
        "client:generate": "nuxt generate",
        "client:lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
        "server:dev": "node_modules/.bin/nodemon",
        "server:start": "ts-node server/src/index.ts",
        "server:lint": "node_modules/.bin/tslint --project tsconfig.json --exclude server/src/migrations/*.ts",
        "test": "jest --runInBand",
        "test:watch": "jest --runInBand --watchAll",
        "typeorm": "node tsnode-typeorm",
        "data:prepare": "cross-env npm run data:download && npm run data:filter",
        "data:download": "ts-node data/downloadDatasets.ts",
        "data:filter": "cross-env npm run data:filter:movies && npm run data:filter:ratings",
        "data:filter:movies": "ts-node data/filterMoviesAndLinks.ts",
        "data:filter:ratings": "ts-node data/filterRatings.ts",
        "data:json:movies": "ts-node data/moviesDataJSON.ts",
        "data:json:ratings": "ts-node data/ratingsDataJSON.ts",
        "data:csv:movies": "ts-node data/moviesDataCSV.ts",
        "data:csv:ratings": "ts-node data/ratingsDataCSV.ts",
        "data:csv:metadata": "ts-node data/moviesMetadataCSV.ts",
        "data:csv:keywords": "ts-node data/moviesKeywordsCSV.ts",
        "db:prepare": "npm run typeorm schema:drop && npm run typeorm migration:run",
        "db:populate": "npm run db:movies && npm run db:ratings",
        "db:movies": "ts-node data/moviesData.ts",
        "db:ratings": "ts-node data/ratingsData.ts"
    },
    "dependencies": {
        "@nuxtjs/auth": "4.5.3",
        "@nuxtjs/axios": "5.3.6",
        "apexcharts": "^3.5.0",
        "app-root-path": "^2.1.0",
        "axios": "^0.18.0",
        "bcrypt": "^3.0.4",
        "bootstrap": "^4.3.1",
        "bootstrap-vue": "^2.0.0-rc.15",
        "config": "^1.31.0",
        "cors": "^2.8.4",
        "csvtojson": "^2.0.8",
        "cwait": "^1.1.1",
        "eboom": "^0.0.23",
        "element-ui": "^2.6.1",
        "express": "^4.16.3",
        "faker": "^4.1.0",
        "fast-csv": "^2.4.1",
        "jest": "^23.4.2",
        "joi": "^13.5.2",
        "jsonwebtoken": "^8.3.0",
        "lodash": "^4.17.10",
        "moment": "^2.22.2",
        "morgan": "^1.9.1",
        "mysql2": "^1.6.1",
        "nuxt": "2.4.3",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "path-to-regexp": "^2.2.1",
        "reflect-metadata": "^0.1.12",
        "supertest": "^3.1.0",
        "ts-jest": "^23.1.2",
        "typeorm": "0.2.7",
        "vue": "^2.6.7",
        "vue-apexcharts": "^1.3.0",
        "vue-truncate-filter": "^1.1.7",
        "vuex": "^3.1.0",
        "winston": "^3.1.0",
        "winston-daily-rotate-file": "^3.8.0"
    },
    "devDependencies": {
        "@types/adm-zip": "^0.4.32",
        "@types/cli-progress": "^1.8.1",
        "@types/cors": "^2.8.4",
        "@types/csvtojson": "^1.1.5",
        "@types/express": "^4.16.0",
        "@types/faker": "^4.1.4",
        "@types/jest": "^23.3.1",
        "@types/joi": "^13.4.0",
        "@types/lodash": "^4.14.116",
        "@types/morgan": "^1.7.35",
        "@types/node": "^10.5.6",
        "@types/passport": "^0.4.6",
        "@types/passport-jwt": "^3.0.1",
        "@types/supertest": "^2.0.5",
        "adm-zip": "^0.4.13",
        "axios-retry": "^3.1.2",
        "babel-eslint": "^8.2.1",
        "cli-progress": "^2.1.1",
        "cross-env": "^5.0.1",
        "eslint": "^5.14.1",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-vue": "^5.2.2",
        "node-sass": "^4.11.0",
        "nodemon": "^1.18.3",
        "sass-loader": "^7.1.0",
        "ts-node": "^7.0.0",
        "tslint": "^5.10.0",
        "tslint-config-airbnb": "^5.9.2",
        "typescript": "^2.9.2"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "testEnvironment": "node",
        "testPathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/server/dist/"
        ],
        "moduleNameMapper": {
            "^@server/(.*)$": "<rootDir>/server/src/$1"
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
